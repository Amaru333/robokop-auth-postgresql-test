datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id           Int          @id @default(autoincrement())
  email        String       @unique
  password     String
  createdAt    DateTime     @default(now())
  savedQueries SavedQuery[] 
}

model SavedQuery {
  id        Int      @id @default(autoincrement())
  user      User     @relation(fields: [userId], references: [id])
  userId    Int
  name      String?   
  query     Json      
  createdAt DateTime  @default(now())
}
